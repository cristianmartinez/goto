                    <h3>To:</h3>
                    <datepicker ng-model="vm.toDate"></datepicker>
                </div>
            </div>
            <div class="footer"><a href="http://cristianmartinez.co">cristianmartinez.co</a></div>
        </div>
    </div>

    <script src="/angular/angular.js"></script>
    <script src="/js/datepicker.api.js"></script>
